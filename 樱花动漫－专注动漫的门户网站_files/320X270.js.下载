(lib._320X270 = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// timeline functions:
	this.frame_0 = function() {
		this.stop();
		var _self=this;
		addVideo(this.videobg,"//sccdn.002lzj.com/swfs/34/4431944181xxs/gfd8fkd.mp4","video");
		
		function addVideo(adLib, src1, id) {
			if (document.getElementById(id)) {
				return;
			}
		
			var elVideo = document.createElement("video");
			elVideo.setAttribute("loop", "loop");
			elVideo.setAttribute("autoplay", "autoplay");
			elVideo.setAttribute("muted", "muted");
		
			_self.bgmc.visible = false;
			elVideo.setAttribute("src", src1);
			document.getElementById("canvas").style.background = "none";
		
			elVideo.setAttribute("id", id);
		
			elVideo.className = "video";
			elVideo.muted = true;
			elVideo.play();
		
			var _width = adLib.nominalBounds.width * (stage.scaleX || 1);
			var _height = adLib.nominalBounds.height * (stage.scaleY || 1);
			var _left = (adLib.x - adLib.regX) * (stage.scaleX || 1);
			var _top = (adLib.y - adLib.regY) * (stage.scaleY || 1);
		
			var _stage = document.getElementById("canvas");
		
			var new_left = _left + _stage.offsetLeft;
			elVideo.setAttribute("style", "width:" + _width + "px;height:" + _height + "px;left:" + new_left + "px;top:" + _top + "px;position:absolute;object-fit: fill;");
		
			_stage.style["z-index"] = 2;
			_stage.style["position"] = "fixed";
			_stage.style["background-color"] = "rgba(255, 255, 255, 0.00)";
		
		
			var elDiv = document.createElement("div");
			elDiv.setAttribute("style", "z-index:1;position:fixed");
			elDiv.setAttribute("id", "videodiv");
			elDiv.appendChild(elVideo);
			document.body.appendChild(elDiv);
			adLib.elVideo = elVideo;
		
		
			window._resizes && window._resizes.push(function () {
		
				var _width = adLib.nominalBounds.width * (stage.scaleX || 1);
				var _height = adLib.nominalBounds.height * (stage.scaleY || 1);
				var _left = (adLib.x - adLib.regX) * (stage.scaleX || 1);
				var _top = (adLib.y - adLib.regY) * (stage.scaleY || 1);
				var new_left = _left + _stage.offsetLeft;
				elVideo.setAttribute("style", "width:" + _width + "px;height:" + _height + "px;left:" + new_left + "px;top:" + _top + "px;position:absolute;object-fit: fill;");
		
		
			})
		
		}
		if(query("hlogo")==1){
			this.logo.visible = false;
		}
		
		function query(name){
		var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
		        var r = window.location.search.substr(1).match(reg);
		        if(r!=null)return  decodeURIComponent(r[2]); return null;
		}
	}

	// actions tween:
	this.timeline.addTween(cjs.Tween.get(this).call(this.frame_0).wait(1));

	// 角标
	this.logo = new lib.gai_1_24_69334();
	this.logo.name = "logo";
	this.logo.parent = this;
	this.logo.setTransform(61.95,165.05);

	this.timeline.addTween(cjs.Tween.get(this.logo).wait(1));

	// 其他内容
	this.instance = new lib.an();
	this.instance.parent = this;
	this.instance.setTransform(108,192);

	this.timeline.addTween(cjs.Tween.get(this.instance).wait(1));

	// 视频
	this.videobg = new lib.元件14();
	this.videobg.name = "videobg";
	this.videobg.parent = this;
	this.videobg.setTransform(0,0,1,0.9999);

	this.timeline.addTween(cjs.Tween.get(this.videobg).wait(1));

	// 图层_2
	this.bgmc = new lib.元件1();
	this.bgmc.name = "bgmc";
	this.bgmc.parent = this;
	this.bgmc.setTransform(160,120,1,1,0,0,0,160,120);

	this.timeline.addTween(cjs.Tween.get(this.bgmc).wait(1));

}).prototype = p = new cjs.MovieClip();
